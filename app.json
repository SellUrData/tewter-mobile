{
  "expo": {
    "name": "Tewter",
    "slug": "tewter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tewter.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Tewter needs camera access to take photos of math problems for solving.",
        "NSPhotoLibraryUsageDescription": "Tewter needs photo library access to upload images of math problems."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tewter.app",
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Tewter to access your camera to take photos of math problems."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Tewter to access your photos to upload math problems."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
